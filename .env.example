# Server Configuration
PORT=3000
NODE_ENV=development
API_BASE_URL=http://localhost:3000

# Rentman API Configuration
RENTMAN_API_TOKEN=your_token_here
RENTMAN_BASE_URL=https://www.rentman.online

# Redis Cache Configuration
UPSTASH_REDIS_URL=redis://localhost:6379
UPSTASH_REDIS_TOKEN=
REDIS_PASSWORD=

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/london_move

# AWS S3 & CDN Configuration
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key
AWS_REGION=us-east-1
S3_BUCKET=london-move-images
CDN_BASE_URL=https://cdn.london-move.com

# Admin Authentication
JWT_SECRET=your_super_secret_jwt_key_here
ADMIN_DEFAULT_EMAIL=admin@london-move.com
ADMIN_DEFAULT_PASSWORD=secure_admin_password

# Performance & Caching
CACHE_PREFIX=lm:
MEMORY_CACHE_SIZE=1000
MEMORY_CACHE_TTL=300000

# Image Processing
IMAGE_QUALITY_DEFAULT=85
IMAGE_MAX_WIDTH=3840
IMAGE_MAX_HEIGHT=2160

# Frontend Integration
FRONTEND_URL=https://stupendous-share-046449.framer.app
CORS_ORIGINS=https://stupendous-share-046449.framer.app,http://localhost:3000

# Monitoring & Logging
LOG_LEVEL=info
ENABLE_REQUEST_LOGGING=true

# Feature Flags
ENABLE_EDGE_CACHING=true
ENABLE_IMAGE_PRECOMPUTATION=true
ENABLE_PREDICTIVE_CACHING=true
ENABLE_AUTO_OPTIMIZATION=true
ENABLE_RESPONSE_PRECOMPUTATION=true